{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://yourproductionurl.com",
        "CACHE_DRIVER": "file",
        "SESSION_DRIVER": "database",
        "SESSION_LIFETIME": "120",
        "LOG_CHANNEL": "stderr"
    }
}
